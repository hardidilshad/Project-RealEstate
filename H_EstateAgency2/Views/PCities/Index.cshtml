@model IEnumerable<H_EstateAgency2.Models.PCity>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PCityName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PCityNo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PCityName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PCityNo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PCityId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PCityId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PCityId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
